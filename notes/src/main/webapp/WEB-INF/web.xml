  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GoogleLoginServlet</servlet-name>
  	<url-pattern>/GoogleLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>OAuthCallbackServlet</servlet-name>
  	<url-pattern>/OAuthCallbackServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>logoutServlet</servlet-name>
  	<url-pattern>/logoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddNotes</servlet-name>
  	<url-pattern>/AddNotes</url-pattern>
  </servlet-mapping>
